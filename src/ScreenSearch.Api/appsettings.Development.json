{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ScreenSearchSettings": {
    "TMDBAPISettings": {
      "BaseURL": "https://api.themoviedb.org/",
      "SearchMoviesPath": "3/search/movie",
      "SearchShowsPath": "3/search/tv",
      "GetMovieByIdPath": "3/movie",
      "GetShowByIdPath": "3/tv",
      "GetTrendingMoviesPath": "3/trending/movie",
      "GetTrendingSeriesPath": "3/trending/tv",
      "GetLanguagesPath": "3/configuration/languages"
    },
    "KinocheckAPISettings": {
      "BaseURL": "https://api.kinocheck.com/",
      "GetMovieVideosPath": "movies",
      "GetSeriesVideosPath": "shows",
      "GetTrendingTrailersPath": "trailers/trending",
      "GetLatestTrailersPath": "trailers/latest"
    },
    "CacheSettings": {
      "CacheExpirationInMinutes": 15
    },
    "TrendingJobSettings": {
      "ExecutionIntervalInMinutes": 60,
      "Language": "en"
    },
    "SupportedLanguagesJobSettings": {
      "ExecutionIntervalInMinutes": 60
    },
    "LanguageSettings": {
      "DefaultCulture": "en",
      "FallbackCulture": "en"
    },
    "RateLimitSettings": {
      "TMDB": {
        "NumberOfRequestsPermitted": 50,
        "WindowSizeInSeconds": 60
      }
    },
    "FeatureManagement": {
      "TrendingJobEnabled": false,
      "SupportedLanguagesJobEnabled": false
    }
  }
}